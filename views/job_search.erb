<div class="container">
  <table class="table">
    <caption> Jobs in <%= user_location["city"] + ", " + user_location["region_name"]%> </caption>
    <thead>
      <tr>
        <th>Title</th>
        <th>Company</th>
        <th>Location</th>
      </tr>
    </thead>

    <tbody>
    <% job_results.each do |job| %>
      <tr>
      <td> <%= job.title %> </td>
      <td> <%= job.company %> </td>
      <td> <%= job.location %> </td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
